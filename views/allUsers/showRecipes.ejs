<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Explore <%= selectedUser.username%>'s Pantry</title>
</head>

<body>
    <%- include('../partials/_navbar.ejs') %>
        <h1>Explore <%= selectedUser.username%>'s Pantry</h1>
        

<ul>
      <% selectedUser.pantry.forEach(food =>{ %>
      <li>
         <a href="/users/<%= user._id %>/foods/<%= food._id %>">
        <%= food.name %>
        </a>
    </li>
     <% }) %>
</ul>

</body>

</html>